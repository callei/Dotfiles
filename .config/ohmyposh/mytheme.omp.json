{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "bg": "#1b1b1a",
    "fg": "#ebe8e3",
    "accent": "#D2C6B9",
    "color0": "#1b1b1a",
    "color1": "#A5A09B",
    "color2": "#C6B9AB",
    "color3": "#D2C6B9",
    "color4": "#BCBEC2",
    "color5": "#D7D0C9",
    "color6": "#E4DBD2",
    "color7": "#ebe8e3",
    "color8": "#a4a29e",
    "color9": "#A5A09B",
    "color10": "#C6B9AB",
    "color11": "#D2C6B9",
    "color12": "#BCBEC2",
    "color13": "#D7D0C9",
    "color14": "#E4DBD2",
    "color15": "#ebe8e3"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:color4",
          "foreground": "p:bg",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "",
          "type": "session"
        },
        {
          "type": "text",
          "style": "plain",
          "template": " ",
          "background": "transparent",
          "foreground": "transparent"
        },
        {
          "background": "p:color2",
          "foreground": "p:bg",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "~",
            "style": "folder"
          },
          "style": "powerline",
          "template": " \uea83  {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:color3",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:color9{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:color1{{ end }}",
            "{{ if gt .Ahead 0 }}p:color11{{ end }}",
            "{{ if gt .Behind 0 }}p:color11{{ end }}"
          ],
          "foreground": "p:bg",
          "leading_diamond": "\ue0b6",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "background": "p:color2",
          "foreground": "p:bg",
          "style": "plain",
          "template": "<p:color2,transparent>\ue0b6</> \uf489 {{ .Name }} ",
          "type": "shell"
        },
        {
          "background": "p:color4",
          "foreground": "p:bg",
          "invert_powerline": true,
          "leading_diamond": "",
          "style": "diamond",
          "template": "<p:color4,p:color2>\ue0b2</> {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 3
}
